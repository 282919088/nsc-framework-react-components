{
    "presets": [
      "@babel/preset-react",
      ["@babel/preset-env", {
        "targets": "> 0.25%, not dead",
        "modules": false
      }]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      ["import", {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": "css" // `style: true` 会加载 less 文件
      }]
    ]
  }